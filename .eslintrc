{
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, {
      "extensions": [".js"]
    }],
    "react/no-unescaped-entities": ["error", {
      "forbid": ["'"]
    }],
    "react/jsx-one-expression-per-line": 0
  },
  "parser": "babel-eslint"
}
